{
    "name": "dmytrof/settings-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SettingsBundle",
    "keywords": ["Settings"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.3",
        "doctrine/orm": "^2.6",
        "symfony/validator": "^4.3",
        "symfony/form": "^4.3",
        "symfony/doctrine-bridge": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
